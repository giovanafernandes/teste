<div class="row window">
    <div class="col-md-12">
        <p id="notice"><%= notice %></p>
        <input type="hidden" id="active" value="4">
        <div class="row">
            <div class="col-6">
                <h5 class="table-title">Obras</h5>
            </div>
            <div class="col-6" style="text-align: right;">
                <%= link_to spreadsheet_work_path, target: "blank", class: "link-plusing", style: "float: right;" do %>
                  <img src="<%= image_url("plus.svg") %>" width="30">
                <% end %>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <%= react_component 'IndexWork', authenticity_token: form_authenticity_token %>
             </div>
        </div>
    </div>
</div>
